{
  "name": "TRiBot Premium Statistics",
  "description": "Adds a new tab to the TRiBot repoistory that displays revenue information about public premium scripts.",
  "version": "2.0",
  "manifest_version": 2,
  "content_scripts": [
    {
      "js": [
        "libs/jquery-3.3.1.min.js",
        "libs/mustache-2.3.0.min.js",
        "libs/Chart-2.7.1.min.js",
        "Calender.js",
        "PurchaseLog.js",
        "main.js"
      ],
      "matches": [
        "https://tribot.org/repository/*"
      ]
    }
  ],
  "web_accessible_resources": [
    "templates/*"
  ]
}